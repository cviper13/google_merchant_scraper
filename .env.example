# Environment variables for Docker
# Copy this file to .env and fill in your credentials

# SFTP Configuration for Google Merchant Center
SFTP_HOST=partnerupload.google.com
SFTP_PORT=19321
SFTP_USERNAME=your_username_here
SFTP_PASSWORD=your_password_here

# Scraping Configuration
SCRAPE_LINKS=true
MAX_WORKERS=8
DELAY=0.1
ENABLE_RETRY=true
UPLOAD_TO_SFTP=true

# Optional: Custom base URL
BASE_URL=https://www.utkuoptik.com

# Optional: Custom category settings
CATEGORY_URL=https://www.utkuoptik.com/kategori/gunes-gozlukleri-54/
TOTAL_PAGES=25
